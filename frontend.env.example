# Frontend Environment Variables
# Copy this to your frontend project as .env.local

# Backend API Base URL
NEXT_PUBLIC_API_BASE_URL=https://ee-openai-agent-production.up.railway.app
# For local development:
# NEXT_PUBLIC_API_BASE_URL=http://localhost:3000

# Authentication Token
# Currently: any token works (format validation only)
# Production: will be provided by backend team
NEXT_PUBLIC_AUTH_TOKEN=any-token

# Optional: Default Agent ID
NEXT_PUBLIC_DEFAULT_AGENT_ID=my-agent

# Optional: Default Folder for Files
NEXT_PUBLIC_DEFAULT_FOLDER=shared
